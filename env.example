# Environment type (mainnet, testnet, or localnet)
# This determines the chain ID
ENV=testnet

# Base Chain RPC URL (required)
# Should be a complete private RPC URL with API key included
# Example: https://base-mainnet.g.alchemy.com/v2/your-api-key
RPC_URL=https://your-rpc-provider.com/your-api-key

# Private key for the wallet that will pay for gas (without 0x prefix)
PRIVATE_KEY=your_private_key_here_without_0x_prefix 

# Sentry DSN
SENTRY_DSN=https://sentry.

# API access token for authentication
BEACONATOR_ACCESS_TOKEN=your_api_token_here

# Admin token for beacon type management endpoints (required)
BEACONATOR_ADMIN_TOKEN=your_admin_token_here

# Redis URL for wallet pool
REDIS_URL=redis://127.0.0.1:6379

# Wallet private keys for the pool (comma-separated, without 0x prefix)
# These wallets are used for sending transactions (beacon creation, perp deployment, etc.)
WALLET_PRIVATE_KEYS=private_key_1,private_key_2,private_key_3

# Optional: Instance ID for wallet locking (auto-generated UUID if not set)
# BEACONATOR_INSTANCE_ID=instance-1

# Contract addresses (replace with actual deployed contract addresses)
# These are used to seed default beacon types in the registry on startup
BEACON_FACTORY_ADDRESS=0x1234567890123456789012345678901234567890
PERPCITY_REGISTRY_ADDRESS=0x3456789012345678901234567890123456789012
PERP_MANAGER_ADDRESS=0x5678901234567890123456789012345678901234

# Optional: Dichotomous beacon factory address (seeds "verifiable-twap" type if set)
# DICHOTOMOUS_BEACON_FACTORY_ADDRESS=0x...

# USDC token address (different for mainnet/testnet)
USDC_ADDRESS=0x833589fCD6eDb6E08f4c7C32D4f71b54bdA02913

# Multicall3 contract address for batch operations (optional)
# Base mainnet: 0xcA11bde05977b3631167028862bE2a173976CA11
# Base testnet: 0xcA11bde05977b3631167028862bE2a173976CA11
MULTICALL3_ADDRESS=0xcA11bde05977b3631167028862bE2a173976CA11

# Transfer limits for guest wallet funding
USDC_TRANSFER_LIMIT=1000000000  # 1000 USDC (6 decimals)
ETH_TRANSFER_LIMIT=10000000000000000  # 0.01 ETH in wei

# Perp Module Addresses (required)
# These modules define the configuration for perp contracts
# Configuration is on-chain in these deployed module contracts
FEES_MODULE_ADDRESS=0x1234567890123456789012345678901234567890
MARGIN_RATIOS_MODULE_ADDRESS=0x2345678901234567890123456789012345678901
LOCKUP_PERIOD_MODULE_ADDRESS=0x3456789012345678901234567890123456789012
SQRT_PRICE_IMPACT_LIMIT_MODULE_ADDRESS=0x4567890123456789012345678901234567890123

# Optional: Default starting price for perp deployment (can be overridden per request)
# Starting square root price in Q96 format (e.g., sqrt(50) * 2^96 = 560227709747861419891227623424)
PERP_DEFAULT_STARTING_SQRT_PRICE_X96=560227709747861419891227623424